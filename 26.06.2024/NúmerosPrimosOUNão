#include <iostream>
using namespace std;
int main()
{
int numero;
cout <<"Digite um número inteiro:";
cin>> numero;

for(int i=2; i<numero; i++){
 
  if(numero% i==0){
   cout<<"O número não é primo."<< std::endl;
    return 0;
}

}
cout<<"O número é primo."<< std::endl;

    return 0;
}