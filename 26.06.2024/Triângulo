 #include <iostream>
 using namespace std;
  int main(){
      

    float lado1, lado2, lado3;
    
    cout<<"Insira o comprimento do lado 1:";
    cin>>lado1;
     cout<<"Insira o comprimento do lado 2:";
    cin>>lado2;
     cout<<"Insira o comprimento do lado 3:";
    cin>>lado3;

    if (lado1 + lado2 > lado3 && lado1 + lado3 > lado2 && lado2 + lado3> lado1){
             cout<<"É um triângulo";
  }
  else 
  {
     cout<<"Não é triângulo.";
  }
  return 0;
}